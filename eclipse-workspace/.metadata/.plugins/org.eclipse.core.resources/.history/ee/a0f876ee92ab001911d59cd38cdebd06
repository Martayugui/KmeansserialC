int main(void) {


	    float *image;
		int pixels;
		int bands;
		int columns;
		int rows;
		bands=128;
	    rows=270;
	    columns=233;
		pixels= columns*rows;
		struct parameters par;

		puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
		void readParAndImg (rows,columns,bands,pixels,&image,&par, "Parameters.txt", "Op4C3.txt");

	return EXIT_SUCCESS;
}
